{
  "name": "conduit-frontend",
  "version": "0.16.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.9.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.1",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/chance": "^1.1.3",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.3",
    "chance": "^1.1.8",
    "chance-object": "^1.0.1",
    "eslint": "^8.14.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "plop": "^3.1.1",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^18.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "CI=false && react-scripts build",
    "test": "react-scripts test --coverage",
    "lint": "eslint src/**/*.js src/**/*.jsx src/**/*.ts src/**/*.tsx",
    "eject": "react-scripts eject",
    "generate": "plop"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "resetMocks": false,
    "coverageReporters": [
      "json",
      "html",
      "lcov",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "src/**/*.{ts,tsx}",
      "!src/**/index.ts",
      "!src/environment.js",
      "!src/**/*.d.ts",
      "!src/**/*.test.js",
      "!src/**/*.stories.jsx",
      "!src/mockReactComponents.jsx",
      "!src/setupTests.js",
      "!src/setupProxy.js",
      "!src/enzyme-test-utils.jsx",
      "!src/serviceWorker.ts",
      "!src/reportWebVitals.ts",
      "!src/TestUtils.ts",
      "!src/testFinders.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
